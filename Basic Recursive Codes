void printNumbers(int n){ // Printing Numbers in Reverse Order .
    if(n==0) return ;
    cout << n << gap ;
    printNumbers(n-1) ;
}

void printNumbers(int n){ // Printing Numbers in Regular Order .
    if(n==0) return ;
    printNumbers(n-1) ;
    cout << n << gap ;  
}

ll factorial(ll n){
    if(n==0) return 1LL ; 
    return n * 1LL * factorial(n-1) ;
}

ll sumOfNums(ll n){
    if(n==0) return 0LL ; 
    return n + sumOfNums(n-1) ;
}
